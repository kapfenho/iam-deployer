#!/bin/sh

${WL_HOME}/common/bin/wlst.sh -loadProperties ${HOME}/.env/identity.prop <<EOS
nmConnect(userConfigFile=nmUC,userKeyFile=nmUK,host=hostname,port=nmPort,domainName=domName,domainDir=domDir,nmType='ssl');
nmStart('AdminServer');
connect(userConfigFile=domUC,userKeyFile=domUK,url=domUrl);
start(name='soa_cluster',type='Cluster',block='true');
start(name='oim_cluster',type='Cluster',block='true');
exit();
EOS

exit 0

