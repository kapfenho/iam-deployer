#!/bin/sh

${WL_HOME}/common/bin/wlst.sh -loadProperties ${HOME}/.env/access.prop <<EOS
nmConnect(userConfigFile=nmUC,userKeyFile=nmUK,host=hostname,port=nmPort,domainName=domName,domainDir=locDir,nmType='ssl');
connect(userConfigFile=domUC,userKeyFile=domUK,url=domUrl);
start(name='wls_oam1',type='Server',block='true');
exit();
EOS

exit 0

