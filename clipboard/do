. ./setenv

on_vm oim1 'cp /vagrant/user-config/hostenv/oim/common.env /home/fmwuser/.env/'
on_vm oim2 'cp /vagrant/user-config/hostenv/oim/common.env /home/fmwuser/.env/'

on_vm oam1 'cp /vagrant/user-config/hostenv/oam/common.env /home/fmwuser/.env/'
on_vm oam2 'cp /vagrant/user-config/hostenv/oam/common.env /home/fmwuser/.env/'

on_vm oud1 'cp /vagrant/user-config/hostenv/dir/common.env /home/fmwuser/.env/'
on_vm oud2 'cp /vagrant/user-config/hostenv/dir/common.env /home/fmwuser/.env/'
on_vm oud2 'sed -i "s/oud1/oud2/" /home/fmwuser/.env/common.env'

on_vm web1 'cp /vagrant/user-config/hostenv/web/common.env /home/fmwuser/.env/'
on_vm web2 'cp /vagrant/user-config/hostenv/web/common.env /home/fmwuser/.env/'
on_vm oud2 'sed -i "s/ohs1/ohs2/" /home/fmwuser/.env/common.env'
