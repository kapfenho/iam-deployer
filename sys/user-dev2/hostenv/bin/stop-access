#!/bin/sh

. ${HOME}/.env/acc.env

${WL_HOME}/common/bin/wlst.sh -loadProperties ${HOME}/.env/access.prop <<EOS
nmConnect(userConfigFile=nmUC,userKeyFile=nmUK,host=hostname,port=nmPort,domainName=domName,domainDir=admDir,nmType='ssl');
connect(userConfigFile=domUC,userKeyFile=domUK,url=domUrl);
try:
  shutdown('wls_oam1',force='true')
except:
  pass

shutdown('AdminServer',force='true');
exit();
EOS

exit 0

