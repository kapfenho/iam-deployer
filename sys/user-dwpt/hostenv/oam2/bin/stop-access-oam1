#!/bin/sh

${WL_HOME}/common/bin/wlst.sh -loadProperties ${HOME}/.env/access.prop <<EOS
nmConnect(userConfigFile=nmUC,userKeyFile=nmUK,host=hostname,port=nmPort,domainName=domName,domainDir=domDir,nmType='ssl');
connect(userConfigFile=domUC,userKeyFile=domUK,url=domUrl);
shutdown(name='wls_oam1',entityType='Server',force='true',block='true',timeOut=600000);
shutdown(name='AdminServer',entityType='Server', force='true',block='true',timeOut=60000);
exit();
EOS

exit 0

