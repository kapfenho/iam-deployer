 wl_home=/appl/iam/fmw/products/identity/wlserver_10.3
    exec=${wl_home}/common/bin/wlst.sh
    host=$(hostname -f)
     dom=IAMGovernanceDomain
    ddir=/appl/iam/fmw/config/domains/IAMGovernanceDomain
adm_port=7101
 nm_port=5556
    keys=/appl/iam/fmw/config/creds
     nmu=$keys/nm/usr
     nmk=$keys/nm/key
      du=$keys/$dom/usr
      dk=$keys/$dom/key
 logfile=/appl/logs/iam/nodemanager/wlst.log

