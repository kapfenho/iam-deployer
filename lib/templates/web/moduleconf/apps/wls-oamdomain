    # -----------------------------------------------------------------
    # Weblogic Admin of IAM domain
    #
    # vi: ft=apache :

    # webogic admin console
    <Location /console>
        SetHandler weblogic-handler
        __WLS_IDMADMINSERVER__
    </Location>

    <Location /consolehelp>
        SetHandler weblogic-handler
        __WLS_IDMADMINSERVER__
    </Location>

    # enterprise manager for access manager
    <Location /em>
        SetHandler weblogic-handler
        __WLS_IDMADMINSERVER__
    </Location>

    # access manager configuration ui
    <Location /oamconsole>
        SetHandler weblogic-handler
        __WLS_IDMADMINSERVER__
    </Location>

    # Oracle Entitlements Server, APM
    <Location /apm>
        SetHandler weblogic-handler
        __WLS_IDMADMINSERVER__
    </Location>
