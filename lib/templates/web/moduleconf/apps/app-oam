    # -----------------------------------------------------------------
    # OAM contexts
    #
    # vi: ft=apache :

    <Location /access>
      SetHandler weblogic-handler
      WebLogicHost iamvs.agoracon.at
      WebLogicPort 14100
      WLCookieName OAMSESSIONID    
      WLProxySSL ON
    </Location>

    <Location /oam>
      SetHandler weblogic-handler
      WebLogicHost iamvs.agoracon.at
      WebLogicPort 14100
      WLCookieName OAM_JSESSIONID
      WLProxySSL ON
    </Location>

    <Location /msmconsole>
      SetHandler weblogic-handler
      WebLogicHost iamvs.agoracon.at
      WebLogicPort 14100
      WLCookieName OAM_JSESSIONID
      WLProxySSL ON
    </Location>

    <Location /oamfed>
      SetHandler weblogic-handler
      WebLogicHost iamvs.agoracon.at
      WebLogicPort 14100
      WLCookieName OAM_JSESSIONID
      WLProxySSL ON
    </Location>

    <Location /ms_oauth>
      SetHandler weblogic-handler
      WebLogicHost iamvs.agoracon.at
      WebLogicPort 14100
      WLCookieName OAMSESSIONID
      WLProxySSL ON
    </Location>

