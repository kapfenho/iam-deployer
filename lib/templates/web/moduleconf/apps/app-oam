    # -----------------------------------------------------------------
    # OAM contexts
    #
    # vi: ft=apache :

    <Location /access>
      SetHandler weblogic-handler
      WLCookieName OAMSESSIONID
      __WLS_OAM__
    </Location>

    <Location /oam>
      SetHandler weblogic-handler
      WLCookieName OAM_JSESSIONID
      __WLS_OAM__
    </Location>

    <Location /msmconsole>
      SetHandler weblogic-handler
      WLCookieName OAM_JSESSIONID
      __WLS_OAM__
    </Location>

    <Location /oamfed>
      SetHandler weblogic-handler
      WLCookieName OAM_JSESSIONID
      __WLS_OAM__
    </Location>

    <Location /ms_oauth>
      SetHandler weblogic-handler
      WLCookieName OAMSESSIONID
      __WLS_OAM__
    </Location>

