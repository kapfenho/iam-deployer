# oracle home: binaries and apps
      APP_SERVER=weblogic
         MW_HOME=${ORACLE_BASE}/products/analytics
         WL_HOME=${MW_HOME}/wlserver_10.3
      RBACX_HOME=${MW_HOME}/oia
     OIM_MW_HOME=${ORACLE_BASE}/products/identity
     OIM_WL_HOME=${OIM_MW_HOME}/wlserver_10.3

export APP_SERVER MW_HOME WL_HOME RBACX_HOME OIM_MW_HOME OIM_WL_HOME 

# java settings
             JAVA_HOME=${ORACLE_BASE}/products/analytics/jdk/current
           JAVA_VENDOR=Oracle
       PRODUCTION_MODE=true
DOMAIN_PRODUCTION_MODE=true
export JAVA_HOME JAVA_VENDOR PRODUCTION_MODE DOMAIN_PRODUCTION_MODE

# instances
        DOMAIN=__DOMAIN_NAME__
   DOMAIN_HOME=${ORACLE_BASE}/services/domains/${DOMAIN}
    DOMAIN_LOG=${LOG_BASE}/${DOMAIN}
      IDM_PROP=~/.env/analytics.prop
export DOMAIN ADMIN_HOME DOMAIN_HOME WRK_HOME DOMAIN_LOG IDM_PROP

# path
PATH=${DPATH}:${JAVA_HOME}/bin
export PATH

# wlst with domain properties
alias       wlst='rlwrap ${WL_HOME}/common/bin/wlst.sh -loadProperties ${IDM_PROP}'
# follow logs
alias nodelogs='tail -F ${LOG_BASE}/nodemanager/*'
alias  srvlogs='tail -F ${DOMAIN_LOG}/AdminServer/AdminServer.out ${DOMAIN_LOG}/wls_soa1/wls_soa1.out ${DOMAIN_LOG}/wls_oim1/wls_oim1.out'

unset       INSTANCE
unset      INST_HOME
unset       INST_LOG

E=oia
export E

deploy() {
  . ${WL_HOME}/server/bin/setWLSEnv.sh
  java weblogic.WLST -loadProperties ~/.env/${1}.prop ~/lib/deploy.py
}

