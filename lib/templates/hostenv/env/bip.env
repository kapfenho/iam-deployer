# path
PATH=${DPATH}:${ORACLE_HOME}/bin
export PATH

eval "$(rbenv init -)"

# wlst with domain properties
alias       wlst='rlwrap ${WL_HOME}/common/bin/wlst.sh -loadProperties ${IDM_PROP}'
alias wlst-imint='rlwrap ${WL_HOME}/common/bin/wlst.sh -loadProperties ~/.env/imint.prop'
# follow logs
alias nodelogs='tail -F /var/log/fmw/nodemanager/*'
alias  srvlogs='tail -F $DOMAIN_LOG/AdminServer/AdminServer.log $DOMAIN_LOG/wls_soa1/wls_soa1.log $DOMAIN_LOG/wls_oim1/wls_oim1.log'

E=idm
export E

deploy() {
  . ${WL_HOME}/server/bin/setWLSEnv.sh
  java weblogic.WLST -loadProperties ~/.env/${1}.prop ${HOME}/lib/deploy.py
}

