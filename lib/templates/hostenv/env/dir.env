# oracle home: binaries and apps
export        MW_HOME=${ORACLE_BASE}/products/dir
export    ORACLE_HOME=${MW_HOME}/oud
unset         WL_HOME
unset OIM_ORACLE_HOME
unset   ORACLE_COMMON
 
# java settings
export              JAVA_HOME=${ORACLE_BASE}/products/dir/__IAM_JDK__
export            JAVA_VENDOR=Oracle
export        PRODUCTION_MODE=true
export DOMAIN_PRODUCTION_MODE=true
unset                ANT_HOME

# instances
export      INSTANCE=oud1
export  INSTANCE_DIR=${ORACLE_BASE}/services/instances/${INSTANCE}
export     INST_HOME=${INSTANCE_DIR}/OUD
export      INST_LOG=${LOG_BASE}/${INSTANCE}
export    DOMAIN_LOG=${INST_LOG}
export   DOMAIN_HOME=${INST_HOME}

# path
export PATH=${DPATH}:${JAVA_HOME}/bin:${INST_HOME}/bin:${ORACLE_HOME}/bin

unset        WL_HOME
unset  ORACLE_COMMON
unset         DOMAIN
unset    DOMAIN_HOME
unset     DOMAIN_LOG

export E=dir

