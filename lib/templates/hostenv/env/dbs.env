# database client env - oracle instant client
#
# you can use it as an independent environment. for adding the client 
# to another env, remove ORACLE_HOME and choose other PATH setting
#
export     ORACLE_HOME=__IAM_TOP__/products/dbs/instantclient_11_2
export         SQLPATH=__IAM_TOP__/products/dbs/instantclient_11_2
export       TNS_ADMIN=__IAM_TOP__/products/dbs/tns
export        NLS_LANG=.AL32UTF8
export LD_LIBRARY_PATH=${SQLPATH}

#export           PATH=${SQLPATH}:${PATH}           # add to existing env
export            PATH=${DPATH}:${ORACLE_HOME}/bin  # independent env

export E=dbs
