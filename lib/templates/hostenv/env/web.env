# oracle home: binaries and apps
export       MW_HOME=${ORACLE_BASE}/products/web
export   ORACLE_HOME=${MW_HOME}/ohs
export ORACLE_COMMON=${MW_HOME}/oracle_common
export     JAVA_HOME=${MW_HOME}/jdk/current

# java settings
export              JAVA_HOME=${MW_HOME}/jdk/current
export            JAVA_VENDOR=Oracle
export        PRODUCTION_MODE=true
export DOMAIN_PRODUCTION_MODE=true
unset                ANT_HOME
 
# instances
export  INSTANCE=ohs1
export INST_HOME=${ORACLE_BASE}/services/instances/${INSTANCE}
export  INST_LOG=${LOG_BASE}/${INSTANCE}/logs

# path
export PATH=${DPATH}:${INST_HOME}/bin:${ORACLE_HOME}/bin:${JAVA_HOME}/bin

export E=web

unset     WL_HOME
unset      DOMAIN
unset DOMAIN_HOME
unset  DOMAIN_LOG

