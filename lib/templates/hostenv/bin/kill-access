#!/bin/sh

. ${HOME}/.env/acc.env

${WL_HOME}/common/bin/wlst.sh -loadProperties ${HOME}/.env/access.prop <<EOS
nmConnect(userConfigFile=nmUC,userKeyFile=nmUK,host=hostname,port=nmPort,domainName=domName,domainDir=domDir,nmType='ssl');
nmKill('AdminServer');
nmKill('wls_oam1');
exit();
EOS

exit 0

