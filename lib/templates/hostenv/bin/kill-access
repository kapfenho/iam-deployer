#!/bin/sh

. ${HOSTENV}/env/acc.env

${WL_HOME}/common/bin/wlst.sh -loadProperties ${HOSTENV}/env/access.prop <<EOS
nmConnect(userConfigFile=nmUC,userKeyFile=nmUK,host=acGetFQDN(),port=nmPort,domainName=domName,domainDir=domDir,nmType='ssl');
nmKill('AdminServer');
nmKill('wls_oam1');
exit();
EOS

exit 0

