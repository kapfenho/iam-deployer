#!/bin/sh

. ${HOSTENV}/env/idm.env

${WL_HOME}/common/bin/wlst.sh -loadProperties ${HOSTENV}/env/identity.prop <<EOS
nmConnect(userConfigFile=nmUC,userKeyFile=nmUK,host=hostname,port=nmPort,domainName=domName,domainDir=domDir,nmType='ssl');
nmKill('wls_oim1');
nmKill('wls_soa1');
nmKill('AdminServer');
exit();
EOS

exit 0

