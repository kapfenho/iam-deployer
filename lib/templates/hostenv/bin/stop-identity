#!/bin/sh

. ~/.env/idm.env

${WL_HOME}/common/bin/wlst.sh -loadProperties ~/.env/identity.prop <<EOS
nmConnect(userConfigFile=nmUC,userKeyFile=nmUK,host=acGetFQDN(),port=nmPort,domainName=domName,domainDir=admDir,nmType='ssl');
connect(userConfigFile=domUC,userKeyFile=domUK,url=domUrl);
try:
  shutdown('wls_oim1',force='true');
except:
  pass

try:
  shutdown('wls_soa1',force='true');
except:
  pass

shutdown('AdminServer',force='true');
exit();
EOS

exit 0

