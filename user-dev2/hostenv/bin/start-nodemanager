#!/bin/sh

NMHOME=/appdata/fmw/config/nodemanager/$(hostname -f)
   LOG=/var/log/fmw/nodemanager/wlst.log

. ${HOME}/.env/acc.sh
cd ${NMHOME}
${WL_HOME}/common/bin/wlst.sh > ${LOG} 2>&1 <<EOS
startNodeManager(verbose='true',NodeManagerHome='${NMHOME}',ListenPort='5556',ListenAddress='$(hostname -f)');
exit()
EOS

exit 0

