#!/bin/sh

LOG=/appl/logs/iam/nodemanager/wlst.log

. ${HOME}/.env/idm.sh

${WL_HOME}/common/bin/wlst.sh > ${LOG} 2>&1 <<EOS
nmConnect('admin','Montag11','$(hostname -f)','5556','IAMGovernanceDomain','/appl/iam/fmw/config/domains/IAMGovernanceDomain','ssl');
nmStart('AdminServer');
nmStart('wls_soa1');
nmStart('wls_oim1');
exit();
EOS

exit 0

